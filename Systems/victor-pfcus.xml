
<PropertyList>

 <!-- Powered Flight Control Units -->
 
 <filter>
  <name>Port Aileron 1</name>
  <type>gain</type>
  <debug>false</debug>
  <input>
   <property>/controls/flight/aileron</property>
  </input>
  <output>
   <property>/systems/PFCUs/port-aileron[0]</property>
  </output>
  <gain>
   <condition>
    <greater-than>
     <property>/systems/electrical/outputs/PFCUs/port-aileron[0]</property>
	 <value>190</value>
	</greater-than>
   </condition>
   <value>0.8</value>
  </gain>
  <gain>
   <condition>
    <less-than>
     <property>/systems/electrical/outputs/PFCUs/port-aileron[0]</property>
	 <value>190</value>
	</less-than>
   </condition>
   <value>0.05</value>
  </gain>
 </filter>
 
