 <filter>
  <name>Port Elevator</name>
  <type>gain</type>
  <debug>false</debug>
  <input>
   <condition>
    <greater-than>
     <property>/systems/electrical/outputs/PFCUs/port-elevator[0]</property>
	 <value>190</value>
	</greater-than>
   </condition>
   <property>/controls/flight/elevator</property>
  <scale>0.6</scale>
  </input>
  <input>
   <condition>
    <less-than>
     <property>/systems/electrical/outputs/PFCUs/port-elevator[0]</property>
	 <value>190</value>
	</less-than>
   </condition>
   <property>/controls/flight/elevator</property>
   <scale>0.05</scale>
  </input>
  <output>
   <property>/systems/PFCUs/port-elevator</property>
  </output>
  <gain>
   <property>/systems/damping/damping-scale</property>
  </gain>
 </filter>

