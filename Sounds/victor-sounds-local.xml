<?xml version="1.0"?>
 
<PropertyList>

 <fx>
 
   <ambience>
   <name>Cockpit Ambience</name>
   <path>Aircraft/Victor/Sounds/cockpit.wav</path>
   <mode>looped</mode>
   <condition>
    <property>/sim/current-view/internal</property>
   </condition>
   <volume>
    <property>/instrumentation/cooling/fan-speed</property>
    <factor>0.2</factor>
    <offset>-0.1</offset>
   </volume>
   
   <pitch>
    <property>/instrumentation/cooling/fan-speed</property>
	<factor>0.7</factor>
	<min>0.3</min>
	<max>1</max>
   </pitch>
   
  </ambience>
  
  <!-- Ground Noise -->
  
  <rumble>
   <name>Ground Rumble Nosegear</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <property>/gear/gear[0]/ground-is-solid</property>
   </condition>
   <volume>
    <property>/gear/gear[0]/rollspeed-ms</property>
    <factor>0.007</factor>
   </volume>
  </rumble>
  
  <rumble>
   <name>Ground Rumble Main Gear Left</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <property>/gear/gear[1]/ground-is-solid</property>
   </condition>
   <volume>
    <property>/gear/gear[1]/rollspeed-ms</property>
    <factor>0.006</factor>
   </volume>
  </rumble>
  
  <rumble>
   <name>Ground Rumble Main Gear Right</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <property>/gear/gear[2]/ground-is-solid</property>
   </condition>
   <volume>
    <property>/gear/gear[2]/rollspeed-ms</property>
    <factor>0.006</factor>
   </volume>
  </rumble>
  
  <!-- Gear -->
  
  <gear>
   <name>Gear</name>
   <mode>in-transit</mode>
   <path>Aircraft/Victor/Sounds/victor-gear.wav</path>
   <property>/gear/gear[0]/position-norm</property>
   
   <pitch>
    <internal>dt_play</internal>
    <offset>1.0</offset>
    <factor>-0.001</factor>
   </pitch>
   
   <volume>
    <property>/sim/current-view/internal</property>
    <factor>-0.3</factor>
    <offset>0.4</offset>
   </volume>

  </gear>
  
  
  
  
