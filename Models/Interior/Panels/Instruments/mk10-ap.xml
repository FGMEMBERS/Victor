<PropertyList>

 <!-- Smiths Mk 10 Autopilot -->
 
 <path>mk10-ap.ac</path>
 
  <effect>
   <inherits-from>Effects/model-transparent</inherits-from>
   <condition>
    <property>/sim/rendering/rembrandt/enabled</property>
   </condition>
  <object-name>ready.lens</object-name>
  <object-name>in.lens</object-name>
  </effect>
 
 <animation> 
  <type>rotate</type>
  <object-name>switch.rudchan</object-name>
  <property>/instrumentation/mk10-autopilot/rudder-channel-switch</property>
  <factor>25</factor>
   <axis>
    <y>-1</y>
   </axis>
   <center>
	<x-m>-0.07187</x-m>
	<y-m>-0.01661</y-m>
	<z-m>0.003575</z-m>
  </center>
 </animation>
 
 <animation> 
  <type>rotate</type>
  <object-name>switch.ailchan</object-name>
  <property>/instrumentation/mk10-autopilot/aileron-channel-switch</property>
  <factor>25</factor>
   <axis>
    <y>-1</y>
   </axis>
   <center>
	<x-m>-0.07187</x-m>
	<y-m>-0.00007</y-m>
	<z-m>0.003575</z-m>
  </center>
 </animation>
 
 <animation> 
  <type>rotate</type>
  <object-name>switch.elchan</object-name>
  <property>/instrumentation/mk10-autopilot/elevator-channel-switch</property>
  <factor>25</factor>
   <axis>
    <y>-1</y>
   </axis>
   <center>
	<x-m>-0.07187</x-m>
	<y-m>0.01682</y-m>
	<z-m>0.003575</z-m>
  </center>
 </animation>
 
 <animation> 
  <type>rotate</type>
  <object-name>bank.select</object-name>
  <property>/instrumentation/mk10-autopilot/bank-selector</property>
  <factor>3</factor>
   <axis>
    <z>-1</z>
   </axis>
   <center>
	<x-m>0</x-m>
	<y-m>0</y-m>
	<z-m>0</z-m>
  </center>
 </animation>
 
 <animation> 
  <type>translate</type>
  <object-name>engage</object-name>
  <property>/instrumentation/mk10-autopilot/engaged-switch</property>
  <factor>0.006</factor>
   <axis>
    <z>1</z>
   </axis>
 </animation>
 
 <animation> 
  <type>translate</type>
  <object-name>power</object-name>
  <property>/instrumentation/mk10-autopilot/power-switch</property>
  <factor>0.006</factor>
   <axis>
    <z>1</z>
   </axis>
 </animation>
 
 <animation> 
  <type>translate</type>
  <object-name>track</object-name>
  <property>/instrumentation/mk10-autopilot/track-switch</property>
  <factor>0.006</factor>
   <axis>
    <z>1</z>
   </axis>
 </animation>
 
  <animation> 
  <type>translate</type>
  <object-name>glide</object-name>
  <property>/instrumentation/mk10-autopilot/glide-switch</property>
  <factor>0.006</factor>
   <axis>
    <z>1</z>
   </axis>
 </animation>
  
 <animation>
  <type>pick</type>
  <object-name>switch.rudchan</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/mk10-autopilot/rudder-channel-switch</property>
   </binding>
  </action>
 </animation>
 
  <animation>
  <type>pick</type>
  <object-name>switch.ailchan</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/mk10-autopilot/aileron-channel-switch</property>
   </binding>
  </action>
 </animation>
 
  <animation>
  <type>pick</type>
  <object-name>switch.elchan</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/mk10-autopilot/elevator-channel-switch</property>
   </binding>
  </action>
 </animation>
 
  <animation>
  <type>pick</type>
  <object-name>power</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/mk10-autopilot/power-switch</property>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>engage</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/mk10-autopilot/engaged-switch</property>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>track</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/mk10-autopilot/track-switch</property>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>glide</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/mk10-autopilot/glide-switch</property>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>bank.0</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>mk10ap.bankselect(0);</script>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>bank.10</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>mk10ap.bankselect(10);</script>
