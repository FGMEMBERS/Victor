 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>bank.-10</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>mk10ap.bankselect(-10);</script>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>bank.20</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>mk10ap.bankselect(20);</script>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>bank.-20</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>mk10ap.bankselect(-20);</script>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>bank.30</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>mk10ap.bankselect(30);</script>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>bank.-30</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>mk10ap.bankselect(-30);</script>
   </binding>
  </action>
 </animation>
 
  <animation>
  <type>pick</type>
  <object-name>bank.40</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>mk10ap.bankselect(40);</script>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>bank.-40</object-name>
  <action>
