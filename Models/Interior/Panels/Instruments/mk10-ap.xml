  </action>
 </animation>
 
 <!-- Pillar Lights -->
 
 <model>
  <path>Aircraft/Victor/Models/Interior/Panels/pillar-light/pillar-light-mk10ap.xml</path>
  <offsets>
   <x-m>-0.0476</x-m>
   <y-m>-0.0389</y-m>
   <z-m>0.012</z-m>
   <heading-deg>120</heading-deg>
  </offsets>
 </model>
 
 <model>
  <path>Aircraft/Victor/Models/Interior/Panels/pillar-light/pillar-light-mk10ap.xml</path>
