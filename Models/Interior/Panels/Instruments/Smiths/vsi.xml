
<PropertyList>

 <path>vsi.ac</path>
 
 <model>
  <path>Aircraft/Victor/Models/Interior/Panels/faceplate/faceplate-std.xml</path>
 </model>
 
 <animation>
  <type>rotate</type>
  <object-name>VSI.needle</object-name>
  <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
  <interpolation>
   <entry>
    <ind>5000</ind>
	<dep>180</dep>
   </entry> 
   <entry>
    <ind>-4000</ind>
	<dep>-147</dep>
   </entry>
   <entry>
    <ind>-2000</ind>
	<dep>-110</dep>
   </entry>
   <entry>
    <ind>-1000</ind>
	<dep>-70</dep>
   </entry>
   <entry>
    <ind>-500</ind>
	<dep>-35</dep>
   </entry>
   <entry>
    <ind>0</ind>
	<dep>0</dep>
   </entry>
   <entry>
    <ind>500</ind>
	<dep>35</dep>
   </entry>
   <entry>
    <ind>1000</ind>
	<dep>70</dep>
   </entry>
   <entry>
    <ind>2000</ind>
	<dep>110</dep>
   </entry>
   <entry>
    <ind>4000</ind>
	<dep>147</dep>
   </entry>   
   <entry>
    <ind>5000</ind>
	<dep>180</dep>
   </entry> 
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center> 
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
</PropertyList>