   <greater-than>
    <property>/systems/electrical/outputs/busbars/port-lv-busbar</property>
	<value>21</value>
   </greater-than>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>AEOs Station Box Off</object-name>
  <object-name>Pilots Station Box Off</object-name>
