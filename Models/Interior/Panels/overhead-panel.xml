
<PropertyList>

 <path>overhead-panel.ac</path>
 
  <animation>
   <type>light</type>
   <light-type>spot</light-type>
   <name>FuelPanelFlood1</name>
   <object-name>flood.1</object-name>
   <nopreview/>
   <position>
     <x>0.4929</x>
     <y>0</y>
     <z>0.6099</z>
   </position>
   <direction>
     <x>-0.575</x>
     <y>0</y>
     <z>-0.75</z>
   </direction>
   <ambient>
     <r>0.03</r>
     <g>0.03</g>
     <b>0.03</b>
     <a>1.0</a>
   </ambient>
   <diffuse>
     <r>0.7</r>
     <g>0.2</g>
     <b>0.2</b>
     <a>1.0</a>
   </diffuse>
   <specular>
     <r>0.8</r>
     <g>0.4</g>
     <b>0.4</b>
     <a>1.0</a>
   </specular>
   <attenuation>
     <c>1.0</c>
     <l>0.002</l>
     <q>0.00005</q>
   </attenuation>
   <dim-factor>
    <property>/controls/rotary/pilots/overhead-panel-floods-dimmer</property>
    <factor>1.25</factor>
    <min>0</min>
    <max>1.5</max>
   </dim-factor>
   <exponent>40.0</exponent>
   <cutoff>40</cutoff>
   <near-m>0.1</near-m>
   <far-m>2</far-m>
 </animation>
 
 <animation> 
  <type>rotate</type>
  <object-name>ignition</object-name>
  <property>/controls/switches/overhead/engine-ignition</property>
  <factor>50</factor>
   <axis>
    <y>-1</y>
   </axis>
   <center>
	<x-m>0.04037</x-m>
	<y-m>-0.08337</y-m>
	<z-m>0.01266</z-m>
  </center>
 </animation>
 
 <animation> 
  <type>rotate</type>
  <object-name>eng-select.1</object-name>
  <property>/controls/switches/overhead/engine-select[0]</property>
  <factor>25</factor>
   <axis>
    <y>-1</y>
   </axis>
   <center>
	<x-m>0.077</x-m>
	<y-m>-0.057</y-m>
	<z-m>0.01266</z-m>
  </center>
 </animation>
 
  <animation> 
  <type>rotate</type>
  <object-name>eng-select.2</object-name>
  <property>/controls/switches/overhead/engine-select[1]</property>
  <factor>25</factor>
   <axis>
    <y>-1</y>
   </axis>
   <center>
	<x-m>0.077</x-m>
	<y-m>-0.03788</y-m>
	<z-m>0.01266</z-m>
  </center>
 </animation>
 
 <animation> 
  <type>rotate</type>
  <object-name>eng-select.3</object-name>
  <property>/controls/switches/overhead/engine-select[2]</property>
  <factor>25</factor>
   <axis>
    <y>-1</y>
   </axis>
   <center>
	<x-m>0.077</x-m>
	<y-m>-0.01740</y-m>
	<z-m>0.01266</z-m>
  </center>
 </animation>
 
 <animation> 
  <type>rotate</type>
  <object-name>eng-select.4</object-name>
  <property>/controls/switches/overhead/engine-select[3]</property>
  <factor>25</factor>
   <axis>
    <y>-1</y>
   </axis>
   <center>
	<x-m>0.077</x-m>
	<y-m>0.00317</y-m>
	<z-m>0.01266</z-m>
  </center>
 </animation>
 
 <animation> 
  <type>rotate</type>
  <object-name>eng-start-master</object-name>
  <property>/controls/switches/overhead/engine-start-master</property>
  <factor>30</factor>
   <axis>
    <y>1</y>
   </axis>
   <center>
	<x-m>0.06447</x-m>
	<y-m>0.06117</y-m>
	<z-m>0.003066</z-m>
  </center>
 </animation>
 
 <animation>
  <type>translate</type>
  <object-name>engine-starter</object-name>
  <property>/controls/buttons/overhead/engine-starter</property>
  <factor>-0.004</factor>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>ignition</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/controls/switches/overhead/engine-ignition</property>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>eng-select.1</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/controls/switches/overhead/engine-select[0]</property>
   </binding>
  </action>
 </animation>
 
  <animation>
  <type>pick</type>
  <object-name>eng-select.2</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/controls/switches/overhead/engine-select[1]</property>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>eng-select.3</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/controls/switches/overhead/engine-select[2]</property>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>eng-select.4</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/controls/switches/overhead/engine-select[3]</property>
   </binding>
  </action>
 </animation>
 
  <animation>
  <type>pick</type>
  <object-name>engine-starter</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
	 <script>controls.enginePanel.EngineStartButton(1)</script>
   </binding>
   <mod-up>
    <binding>
	 <command>nasal</command>
	  <script>controls.enginePanel.EngineStartButton(0)</script>
    </binding>
   </mod-up>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>eng-start-master</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-cycle</command>
    <property>/controls/switches/overhead/engine-start-master</property>
	<value>3</value>
	<value>2</value>
	<value>1</value>
	<value>0</value>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>start-light.1.lens</object-name>
  <condition>
   <property>/engines/engine[0]/start-in-progress</property>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>start-light.1.lens.on</object-name>
  <condition>
   <not>
    <property>/engines/engine[0]/start-in-progress</property>
   </not>
  </condition>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>start-light.2.lens</object-name>
  <condition>
   <property>/engines/engine[1]/start-in-progress</property>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>start-light.2.lens.on</object-name>
  <condition>
   <not>
    <property>/engines/engine[1]/start-in-progress</property>
   </not>
  </condition>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>start-light.3.lens</object-name>
  <condition>
   <property>/engines/engine[2]/start-in-progress</property>
  </condition>
 </animation>
 <animation>
  <type>select</type>
  <object-name>start-light.3.lens.on</object-name>
  <condition>
