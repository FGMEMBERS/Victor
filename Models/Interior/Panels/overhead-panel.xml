  <condition>
   <not>
    <property>/engines/engine[3]/start-in-progress</property>
   </not>
  </condition>
 </animation>
 
 <animation>
  <type>material</type>
  <object-name>start-light.1.lens</object-name>
  <object-name>start-light.2.lens</object-name>
  <object-name>start-light.3.lens</object-name>
  <object-name>start-light.4.lens</object-name>
  <emission>
    <red>1</red>
    <green>0.8</green>
    <blue>0.8</blue>
  </emission>
