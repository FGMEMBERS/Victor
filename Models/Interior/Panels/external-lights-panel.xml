 <animation> 
  <type>rotate</type>
  <object-name>strobe.switch</object-name>
  <property>/controls/switches/pilots/lights/strobes</property>
  <factor>30</factor>
   <axis>
    <y>1</y>
   </axis>
   <center>
	<x-m>0.009368</x-m>
	<y-m>-0.07974</y-m>
	<z-m>-0.04958</z-m>
  </center>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>strobe.switch</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/controls/switches/pilots/lights/strobes</property>
   </binding>
  </action>
 </animation>
 
