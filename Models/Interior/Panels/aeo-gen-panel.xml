  <!-- A-Breaker Control -->
  
 <animation>
  <object-name>alt1.cb.switch</object-name>
  <type>rotate</type>
  <property>/controls/switches/AEOs/engine-alternator[0]</property>
  <factor>90</factor>
  <center>
   <x-m>0.06973</x-m>
   <y-m>-0.11911</y-m>
   <z-m>0.16965</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>alt1.cb.on</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>genpanel.alt_switch(1,1);</script>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>alt1.cb.off</object-name>
