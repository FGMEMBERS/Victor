   <heading-deg>180</heading-deg>
  </offsets>
 </model>
 
 <animation>
  <object-name>ADF Tuner</object-name>
  <type>scale</type>
  <x-offset>0.9</x-offset>
  <y-offset>0.9</y-offset>
  <z-offset>0.9</z-offset>
 </animation>
 
 <model>
  <name>ADF Tuner</name>
  <path>Aircraft/victor/Models/Interior/Panels/adf-tuner-panel.xml</path>
  <offsets>
   <x-m>-0.12</x-m>
